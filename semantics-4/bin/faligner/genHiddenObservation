#!/bin/bash

######################################################################################
# train
######################################################################################
$SRC_DIR/genHiddenObservationGivenWC1C2C3C4Stc.py --dirIn=$DATA_TRN/ho \
        --dirOut=$DATA_TRN \
        --conceptMap=$CONCEPT_MAP \
        --wordMap=$S1_MAP
# train - txt
$SRC_DIR/genHiddenObservationGivenWC1C2C3C4Stc.py --dirIn=$DATA_TXT_TRN/ho \
        --dirOut=$DATA_TXT_TRN \
        --conceptMap=$CONCEPT_MAP_TXT \
        --wordMap=$S1_MAP_TXT
