mkdir -p $DATA_TRN/ho
mkdir -p $DATA_HLDT
mkdir -p $DATA_HLDT/ho
mkdir -p $DATA_TST
mkdir -p $DATA_TST/ho

mkdir -p $DATA_TXT_TRN
mkdir -p $DATA_TXT_TRN/ho
mkdir -p $DATA_TXT_HLDT
mkdir -p $DATA_TXT_HLDT/ho
mkdir -p $DATA_TXT_TST
mkdir -p $DATA_TXT_TST/ho

