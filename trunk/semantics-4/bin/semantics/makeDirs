mkdir -p $MAP_DIR
mkdir -p $MAP_TXT_DIR
mkdir -p $GEN_DIR
mkdir -p $GEN_DIR/tri
mkdir -p $GEN_DIR/trn
mkdir -p $GEN_DIR/smth
mkdir -p $GEN_DIR/smth-txt
